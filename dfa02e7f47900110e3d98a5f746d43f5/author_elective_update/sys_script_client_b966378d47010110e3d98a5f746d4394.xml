        <messages/>
        <name>Change State </name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
 
	var changestate = g_form.getValue('state');

    function statepending() {
   var changestate = g_form.getValue('state');
		if (changestate == -5 )
            g_form.setValue('state', 'Pending');

    }

    function stateworkinprogress() {
      var changestate = g_form.getValue('state');
		if(changestate == 1)
		g_form.setValue('state', 'Work_In_Progress');
     
    }

    function closedcomplete() {
        g_form.setValue('state', 'Closed_Complete');
        return;
    }
}


]]></script>
